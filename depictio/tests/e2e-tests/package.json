{
  "name": "e2e-tests",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "test:headed": "cypress run --headed --no-exit",
    "test:ui": "cypress open",
    "test:auth": "cypress run --spec 'cypress/e2e/auth/standard/**/*.cy.js'",
    "test:public": "CYPRESS_UNAUTHENTICATED_MODE=true cypress run --spec 'cypress/e2e/auth/public/**/*.cy.js'",
    "test:demo": "CYPRESS_DEMO_MODE=true cypress run --spec 'cypress/e2e/auth/demo/**/*.cy.js'",
    "test:single-user": "CYPRESS_SINGLE_USER_MODE=true cypress run --spec 'cypress/e2e/auth/single-user/**/*.cy.js'",
    "test:dashboards": "cypress run --spec 'cypress/e2e/dashboards/**/*.cy.js'",
    "test:ui-themes": "cypress run --spec 'cypress/e2e/ui/**/*.cy.js'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "cypress": "^14.3.3",
    "js-yaml": "^4.1.0"
  }
}
