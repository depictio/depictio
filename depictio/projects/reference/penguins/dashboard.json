      },
      "code_content": "import plotly.graph_objects as go\ntry:\n    from plotly.subplots import make_subplots\nexcept ImportError:\n    import plotly.tools as tls\n    make_subplots = tls.make_subplots\n\n# Custom color palette for islands\nisland_colors = {\n    'Biscoe': '#6366f1',      # Indigo\n    'Dream': '#f59e0b',       # Amber\n    'Torgersen': '#10b981'    # Emerald\n}\n\n# Create subplots: 2 rows, 2 columns\nfig = make_subplots(\n    rows=2, cols=2,\n    subplot_titles=(\n        'Body Mass Distribution by Island',\n        'Flipper Length vs Bill Length',\n        'Bill Dimensions Comparison',\n        'Morphological Summary by Island'\n    ),\n    specs=[\n        [{'type': 'box'}, {'type': 'scatter'}],\n        [{'type': 'scatter'}, {'type': 'bar'}]\n    ],\n    vertical_spacing=0.12,\n    horizontal_spacing=0.10\n)\n\n# Plot 1: Box plot of body mass by island\nfor island in df['island'].unique():\n    island_data = df[df['island'] == island]\n    fig.add_trace(\n        go.Box(\n            y=island_data['body_mass_g'],\n            name=island,\n            marker_color=island_colors[island],\n            boxmean='sd',\n            showlegend=True\n        ),\n        row=1, col=1\n    )\n\n# Plot 2: Scatter of flipper vs bill length colored by island\nfor island in df['island'].unique():\n    island_data = df[df['island'] == island]\n    fig.add_trace(\n        go.Scatter(\n            x=island_data['bill_length_mm'],\n            y=island_data['flipper_length_mm'],\n            mode='markers',\n            name=island,\n            marker=dict(\n                color=island_colors[island],\n                size=8,\n                opacity=0.7,\n                line=dict(width=1, color='white')\n            ),\n            showlegend=False\n        ),\n        row=1, col=2\n    )\n\n# Plot 3: Bill length vs depth with custom styling\nfor island in df['island'].unique():\n    island_data = df[df['island'] == island]\n    fig.add_trace(\n        go.Scatter(\n            x=island_data['bill_length_mm'],\n            y=island_data['bill_depth_mm'],\n            mode='markers',\n            name=island,\n            marker=dict(\n                color=island_colors[island],\n                size=island_data['body_mass_g'] / 400,\n                opacity=0.6,\n                line=dict(width=1.5, color='white'),\n                symbol='diamond'\n            ),\n            showlegend=False\n        ),\n        row=2, col=1\n    )\n\n# Plot 4: Average measurements by island\nisland_summary = df.groupby('island').agg({\n    'body_mass_g': 'mean',\n    'flipper_length_mm': 'mean',\n    'bill_length_mm': 'mean',\n    'bill_depth_mm': 'mean'\n}).reset_index()\n\nmeasurements = ['body_mass_g', 'flipper_length_mm', 'bill_length_mm', 'bill_depth_mm']\nmeasurement_labels = ['Body Mass (g)', 'Flipper (mm)', 'Bill Length (mm)', 'Bill Depth (mm)']\n\nfor i, (measurement, label) in enumerate(zip(measurements, measurement_labels)):\n    # Normalize to 0-1 scale for visualization\n    values = island_summary[measurement]\n    normalized = (values - values.min()) / (values.max() - values.min()) * 100\n    \n    fig.add_trace(\n        go.Bar(\n            x=island_summary['island'],\n            y=normalized,\n            name=label,\n            marker_color=['#ef4444', '#3b82f6', '#22c55e', '#f59e0b'][i],\n            opacity=0.8,\n            showlegend=True\n        ),\n        row=2, col=2\n    )\n\n# Update layout\nfig.update_xaxes(title_text='Island', row=1, col=1)\nfig.update_yaxes(title_text='Body Mass (g)', row=1, col=1)\n\nfig.update_xaxes(title_text='Bill Length (mm)', row=1, col=2)\nfig.update_yaxes(title_text='Flipper Length (mm)', row=1, col=2)\n\nfig.update_xaxes(title_text='Bill Length (mm)', row=2, col=1)\nfig.update_yaxes(title_text='Bill Depth (mm)', row=2, col=1)\n\nfig.update_xaxes(title_text='Island', row=2, col=2)\nfig.update_yaxes(title_text='Normalized Scale (%)', row=2, col=2)\n\nfig.update_layout(\n    title_text='Palmer Penguins: Island-Based Morphological Analysis',\n    title_font_size=18,\n    height=650,\n    showlegend=True,\n    legend=dict(\n        orientation='h',\n        yanchor='bottom',\n        y=-0.15,\n        xanchor='center',\n        x=0.5\n    ),\n    plot_bgcolor='rgba(0,0,0,0)',\n    paper_bgcolor='rgba(0,0,0,0)',\n    font=dict(size=11)\n)\n\nfig.update_layout(barmode='group')\n\nfig"
